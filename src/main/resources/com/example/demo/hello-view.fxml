    <Label fx:id="welcomeText" style=" -fx-border-color:black; -fx-border-width: 1; -fx-border-style: solid;" />

    <HBox>
        <children>
            <Label fx:id="cell_00">
                <style>
                    -fx-border-color:black;
                    -fx-border-width: 1;
                    -fx-border-style: solid;
